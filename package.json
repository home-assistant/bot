{
  "name": "bot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engineStrict": true,
  "engines": {
    "node": ">=14.14.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@golevelup/nestjs-stripe": "^0.1.0",
    "@golevelup/nestjs-webhooks": "^0.2.6",
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/platform-ws": "^7.4.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/websockets": "^7.4.4",
    "@octokit/auth-app": "^2.10.6",
    "@octokit/core": "^3.2.5",
    "@octokit/request": "^5.4.13",
    "@octokit/types": "^6.5.1",
    "@sentry/integrations": "^5.27.1",
    "@sentry/node": "^5.27.1",
    "apollo-server": "^2.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "convict": "^6.0.0",
    "discord-nestjs": "^0.3.6",
    "discord.js": "^12.5.1",
    "nestjs-pino": "^1.2.0",
    "nodemailer": "^6.4.14",
    "pino-pretty": "^4.3.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "semver": "^7.3.2",
    "tslib": "^2.0.3",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/jest": "26.0.14",
    "@types/node": "^14.14.3",
    "@types/ramda": "^0",
    "@types/semver": "^7",
    "@types/source-map-support": "^0",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.5.2",
    "prettier": "^2.1.2",
    "source-map-support": "^0.5.19",
    "supertest": "^5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/sentry/(.*)": "<rootDir>/libs/sentry/src/$1",
      "@app/sentry": "<rootDir>/libs/sentry/src",
      "@app/github/(.*)": "<rootDir>/libs/github/src/$1",
      "@app/github": "<rootDir>/libs/github/src",
      "@app/health/(.*)": "<rootDir>/libs/health/src/$1",
      "@app/health": "<rootDir>/libs/health/src",
      "@app/version/(.*)": "<rootDir>/libs/version/src/$1",
      "@app/version": "<rootDir>/libs/version/src"
    }
  }
}
